<div class="component component-typography">
    <div class="font-families">
        {{#each typography}}
        <div class="font-family {{ font-family }}">
            <h6>{{ title }}</h6>
            <code>${{ font-family }}</code>
            <p>
                Aa Bb Cc Dd Ee Ff
                Gg Hh Ii Jj Kk Ll
                Mm Nn Oo Pp Qq Rr
                Ss Tt Uu Vv Ww Xx
                Yy Zz
            </p>
        </div>
        {{/each}}
    </div>
</div>
